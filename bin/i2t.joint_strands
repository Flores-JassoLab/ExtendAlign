#!/bin/execlineb -P
multisubstitute {
	importas	${JOINT_STRANDS}	${JOINT_STRANDS}
	importas	${REQUERY_MINUS_STRAND}	${REQUERY_MINUS_STRAND}
	importas	${SPLIT_STRANDS}	${SPLIT_STRANDS}
}
pipeline { change-path	${REQUERY_MINUS_STRAND}	${JOINT_STRANDS} }
pipeline { change-path	${SPLIT_STRANDS}	${JOINT_STRANDS} }
pipeline { change-ext	".minus.txt"	".txt" }
change-ext	".plus.txt"	".txt"
