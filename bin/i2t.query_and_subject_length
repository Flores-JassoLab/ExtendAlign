#!/bin/execlineb -P
multisubstitute {
	importas	${QUERY_AND_SUBJECT_LENGTH}	${QUERY_AND_SUBJECT_LENGTH}
	importas	${QUERY_LENGTH}	${QUERY_LENGTH}
	importas	${SUBJECT_LENGTH}	${SUBJECT_LENGTH}
	importas	${BEST_BLAST_ALIGNMENT}	${BEST_BLAST_ALIGNMENT}
}
pipeline { change-path	${QUERY_LENGTH}	${QUERY_AND_SUBJECT_LENGTH} }
pipeline { change-path	${SUBJECT_LENGTH}	${QUERY_AND_SUBJECT_LENGTH} }
change-path	${BEST_BLAST_ALIGNMENT}	${QUERY_AND_SUBJECT_LENGTH}
