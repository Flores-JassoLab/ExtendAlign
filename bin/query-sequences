#!/bin/execlineb -S0
multisubstitute {
	importas prereq 1
	importas REFERENCE 2
	importas NT NT
}
blastn
	-query ${QUERY}
	-db ${SUBJECT}
	-task blastn
	-reward 1
	-penalty -1
	-gapopen 2
	-gapextend 2
	-word_size 7
	-dust no
	-soft_masking false
	-max_target_seqs 100
	-num_threads ${NT}
	-evalue 10
	-outfmt "6 qseqid sseqid pident length mismatch gapopen qstart qend sstart send evalue bitscore sstrand"
